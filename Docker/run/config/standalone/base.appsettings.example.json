{
	"Logging": {
		"LogLevel": {
			"Default": "Warning",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information",
			"Microsoft.EntityFramework": "Warning",
			"System.IO": "Information",
			"SinusSynchronousServices": "Information",
			"SinusSynchronousShared": "Information",
			"SinusSynchronousStaticFilesServer": "Information",
			"SinusSynchronousServer": "Information",
			"SinusSynchronousServer.Services.SystemInfoService": "Warning"
		},
		"File": {
			"BasePath": "logs",
			"FileAccessMode": "KeepOpenAndAutoFlush",
			"FileEncodingName": "utf-8",
			"DateFormat": "yyyMMdd",
			"MaxFileSize": 104857600,
			"Files": [
				{
					"Path": "<date:yyyy>/<date:MM>/<date:dd>/sinus-<date:HH>-<counter:0000>.log"
				}
			]
		}
	},
	"SinusSynchronous": {
		"DbContextPoolSize": 512,
		"RedisPool": 50
	},
	"AllowedHosts": "*",
	"Kestrel": {},
	"IpRateLimiting": {},
	"IPRateLimitPolicies": {}
}
